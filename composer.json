{
    "name": "intoegy/sms",
    "description": "Send SMS from your application",
    "license": "MIT",
    "require": {
        "php": ">=5.4",
        "laravel/framework": ">=5.4"
    },
    "atoload":{
        "psr-4":{
            "Intoegy\\SMS": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "Intoegy\\SMS\\SMSServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
    }
}
